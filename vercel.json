{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://dealstudio-backend.fly.dev/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/blinka",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "landbasedstrategies.com" }],
      "destination": "https://www.landbasedstrategies.com/$1",
      "permanent": true
    }
  ]
}
